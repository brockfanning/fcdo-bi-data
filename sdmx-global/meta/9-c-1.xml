    <gen:Report id="SDG_META_RPT">
      <gen:Target id="KEY_TARGET">
        <gen:ReferenceValue id="DIMENSION_DESCRIPTOR_VALUES_TARGET">
          <gen:DataKey>
            <com:KeyValue id="SERIES">
              <com:Value>IT_MOB_2GNTWK</com:Value>
            </com:KeyValue>
            <com:KeyValue id="REPORTING_TYPE">
              <com:Value>N</com:Value>
            </com:KeyValue>
            <com:KeyValue id="REF_AREA">
              <com:Value>BI</com:Value>
            </com:KeyValue>
          </gen:DataKey>
        </gen:ReferenceValue>
        <gen:ReferenceValue id="Dataflow">
          <gen:ObjectReference>
            <URN>urn:sdmx:org.sdmx.infomodel.datastructure.Dataflow=IAEG-SDGs:DF_SDG_GLC(1.0)</URN>
          </gen:ObjectReference>
        </gen:ReferenceValue>
      </gen:Target>
      <gen:AttributeSet>
        
        <gen:ReportedAttribute id="SDG_GOAL">
          
          <com:Text xml:lang="en"><![CDATA[Objective 9. Build a resilient infrastructure, promote sustainable industrialization that benefits everyone and encourage innovation]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[9.c Significantly increase access to information and communications technologies and ensure that all inhabitants of the least developed countries have access to the Internet at an affordable cost by 2020]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[9.c.1 Proportion of the population with access to a mobile network, by type of technology]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="META_LAST_UPDATE">
          
          <com:Text xml:lang="en"><![CDATA[5/1/2020 12:00:00 AM]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_ORGANISATION">
          
          <com:Text xml:lang="en"><![CDATA[Communication and Telecommunications Regulatory Agency]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_NAME">
          
          <com:Text xml:lang="en"><![CDATA[BIGIRIMANA appollinaire]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="ORGANISATION_UNIT">
          
          <com:Text xml:lang="en"><![CDATA[ARCT statistical service]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_FUNCT">
          
          <com:Text xml:lang="en"><![CDATA[SDG focal point at ARCT]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_EMAIL">
          
          <com:Text xml:lang="en"><![CDATA[apollinaire.bigi@gmail.com]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[It is the percentage of inhabitants living within range of a mobile cellular signal, whether or not they are subscribers or users of mobile phones.]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SOURCE_TYPE">
          
          <com:Text xml:lang="en"><![CDATA[Administrative data]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COLL_METHOD">
          
          <com:Text xml:lang="en"><![CDATA[data collected daily]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="FREQ_COLL">
          
          <com:Text xml:lang="en"><![CDATA[every year]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="REL_CAL_POLICY">
          
          <com:Text xml:lang="en"><![CDATA[annually]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_SOURCE">
          
          <com:Text xml:lang="en"><![CDATA[ARCT]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COMPILING_ORG">
          
          <com:Text xml:lang="en"><![CDATA[ARCT]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="INST_MANDATE">
          
          <com:Text xml:lang="en"><![CDATA[The statistical service and planning of state structures are responsible for compiling data from their state structures for the calculation of indicators at the national level.]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="RATIONALE">
          
          <com:Text xml:lang="en"><![CDATA[the indicator is relevant because it reflects the percentage of subscription]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="REC_USE_LIM">
          
          <com:Text xml:lang="en"><![CDATA[Completeness in the collection of data related to this indicator is difficult because several people can share the same phone]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[This indicator is calculated by dividing the number of inhabitants within range of a mobile cellular signal by the total population and multiplying by 100.]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_VALIDATION">
          
          <com:Text xml:lang="en"><![CDATA[Internal validation and by the technical committee for statistical information (CTIS)]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[Data are available at national level and by network (Mobile networks; Internet)]]></com:Text>
          
        </gen:ReportedAttribute>
        
      </gen:AttributeSet>
    </gen:Report>
  </mes:MetadataSet>
  
