  <mes:MetadataSet structureRef="MDS1" setID="d5f87851-eee9-4d20-ad5c-990f8cadf0b7">
    <com:Name xml:lang="en">SDG METADATA SET</com:Name>
    <gen:Report id="SDG_META_RPT">
      <gen:Target id="KEY_TARGET">
        <gen:ReferenceValue id="DIMENSION_DESCRIPTOR_VALUES_TARGET">
          <gen:DataKey>
            <com:KeyValue id="SERIES">
              <com:Value>DC_ODA_POVLG</com:Value>
            </com:KeyValue>
            <com:KeyValue id="REPORTING_TYPE">
              <com:Value>N</com:Value>
            </com:KeyValue>
            <com:KeyValue id="REF_AREA">
              <com:Value>BI</com:Value>
            </com:KeyValue>
          </gen:DataKey>
        </gen:ReferenceValue>
        <gen:ReferenceValue id="Dataflow">
          <gen:ObjectReference>
            <URN>urn:sdmx:org.sdmx.infomodel.datastructure.Dataflow=IAEG-SDGs:DF_SDG_GLC(1.0)</URN>
          </gen:ObjectReference>
        </gen:ReferenceValue>
      </gen:Target>
      <gen:AttributeSet>
        
        <gen:ReportedAttribute id="SDG_GOAL">
          
          <com:Text xml:lang="en"><![CDATA[Goal 1. Eradicate poverty in all its forms and everywhere]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[1.a Ensure a significant mobilization of resources from multiple sources, including through the strengthening of cooperation for development, in order to endow developing countries, in particular the least developed countries, with adequate and predictable means implement programs and policies aimed at ending poverty in all its forms]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[1.a.1.Proportion of resources directly allocated by the State to poverty reduction programs]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_ORGANISATION">
          
          <com:Text xml:lang="en"><![CDATA[MFBCD]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_NAME">
          
          <com:Text xml:lang="en"><![CDATA[IRAMBONA Eric]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_PHONE">
          
          <com:Text xml:lang="en"><![CDATA[79311134]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_EMAIL">
          
          <com:Text xml:lang="en"><![CDATA[eric.irambona85@gmail.com]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[This is the share of the state budget allocated to poverty reduction programs]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="UNIT_MEASURE">
          
          <com:Text xml:lang="en"><![CDATA[%]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SOURCE_TYPE">
          
          <com:Text xml:lang="en"><![CDATA[Ministry of Finance, Budget and Development Cooperation]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COLL_METHOD">
          
          <com:Text xml:lang="en"><![CDATA[Data is collected from administrative documents]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="FREQ_COLL">
          
          <com:Text xml:lang="en"><![CDATA[Collection is done annually]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="REL_CAL_POLICY">
          
          <com:Text xml:lang="en"><![CDATA[Every year]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_SOURCE">
          
          <com:Text xml:lang="en"><![CDATA[MFBCD]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COMPILING_ORG">
          
          <com:Text xml:lang="en"><![CDATA[MFBCD]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="INST_MANDATE">
          
          <com:Text xml:lang="en"><![CDATA[The Ministerial departments are the members of the SSN whose mission is to collect, process and disseminate the data of their sector.]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="RATIONALE">
          
          <com:Text xml:lang="en"><![CDATA[The indicator is relevant because it reflects the distribution of the state budget.]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="REC_USE_LIM">
          
          <com:Text xml:lang="en"><![CDATA[The update of the indicator requires an exhaustive collection of all government expenditure]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[Budget allocated to poverty reduction programs from the State budget multiplied by 100]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_VALIDATION">
          
          <com:Text xml:lang="en"><![CDATA[The data is validated at two levels: internal validation and validation at the CTIS level before publication]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[The indicator is available at national level.]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COMPARABILITY">
          
          <com:Text xml:lang="en"><![CDATA[Data can be compared to past data]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="OTHER_DOC">
          
          <com:Text xml:lang="en"><![CDATA[Budget Usage Report]]></com:Text>
          
        </gen:ReportedAttribute>
        
      </gen:AttributeSet>
    </gen:Report>
  </mes:MetadataSet>
  
