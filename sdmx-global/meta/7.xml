          <com:Text xml:lang="en"><![CDATA[The numerator may not be exhaustive because some households supplied with electricity are not registered with REGIDESO. The indicator cannot be calculated by gender.]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[Ratio of the population that can be connected to electricity and the total population, expressed as a percentage]]></com:Text>
          
        </gen:ReportedAttribute>
        
