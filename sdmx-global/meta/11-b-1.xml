      <gen:AttributeSet>
        
        <gen:ReportedAttribute id="SDG_GOAL">
          
          <com:Text xml:lang="en"><![CDATA[Goal 11. Make cities and human settlements inclusive, safe, resilient and sustainable]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[11.b By 2020, significantly increase the number of cities and human settlements that adopt and implement integrated policies and action plans for the inclusion of all and the rational use of resources climate change adaptation and mitigation and disaster resilience, and develop and implement, in line with the Sendai Framework for Disaster Risk Reduction (2015-2030), comprehensive management disaster risk at all levels]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[11.b.1 Number of countries that have adopted and put in place national disaster risk reduction strategies, in line with the Sendai Framework for Disaster Risk Reduction (2015-2030)]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="META_LAST_UPDATE">
          
          <com:Text xml:lang="en"><![CDATA[5/1/2020 12:00:00 AM]]></com:Text>
          
        </gen:ReportedAttribute>
        
