      <gen:AttributeSet>
        
        <gen:ReportedAttribute id="SDG_GOAL">
          
          <com:Text xml:lang="en"><![CDATA[Goal 3. Ensure healthy lives and promote well-being for all at all ages]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[3.b Support research and development of vaccines and drugs against diseases, communicable or not, which mainly affect the inhabitants of developing countries, provide affordable access to essential drugs and vaccines, in accordance with the Declaration Doha Round of the Agreement on Trade-Related Aspects of Intellectual Property Rights (TRIPS) and Public Health, which reaffirms the right of developing countries to take full advantage of the flexibility provisions of this agreement when it comes to protecting public health and, in particular, ensuring universal access to medicines]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[3.b.2. Total net amount of official development assistance devoted to medical research and basic health care]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[It is the proportion of public expenditure allocated to health research to total public expenditure on health.]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[Ratio of public spending on health research to total public spending on health, expressed as a percentage.]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[Not available]]></com:Text>
          
        </gen:ReportedAttribute>
        
      </gen:AttributeSet>
    </gen:Report>
  </mes:MetadataSet>
  
