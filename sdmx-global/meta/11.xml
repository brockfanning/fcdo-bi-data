    <gen:Report id="SDG_META_RPT">
      <gen:Target id="KEY_TARGET">
        <gen:ReferenceValue id="DIMENSION_DESCRIPTOR_VALUES_TARGET">
          <gen:DataKey>
            <com:KeyValue id="SERIES">
              <com:Value>SG_DSR_SILS</com:Value>
            </com:KeyValue>
            <com:KeyValue id="REPORTING_TYPE">
              <com:Value>N</com:Value>
            </com:KeyValue>
            <com:KeyValue id="REF_AREA">
              <com:Value>BI</com:Value>
            </com:KeyValue>
          </gen:DataKey>
        </gen:ReferenceValue>
        <gen:ReferenceValue id="Dataflow">
          <gen:ObjectReference>
            <URN>urn:sdmx:org.sdmx.infomodel.datastructure.Dataflow=IAEG-SDGs:DF_SDG_GLC(1.0)</URN>
          </gen:ObjectReference>
        </gen:ReferenceValue>
      </gen:Target>
      <gen:AttributeSet>
        
        <gen:ReportedAttribute id="SDG_GOAL">
          
          <com:Text xml:lang="en"><![CDATA[Goal 11. Make cities and human settlements inclusive, safe, resilient and sustainable]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[11.b By 2020, significantly increase the number of cities and human settlements that adopt and implement integrated policies and action plans for the inclusion of all and the rational use of resources climate change adaptation and mitigation and disaster resilience, and develop and implement, in line with the Sendai Framework for Disaster Risk Reduction (2015-2030), comprehensive management disaster risk at all levels]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[11.b.2 Proportion of local administrations that have adopted and put in place local disaster risk reduction strategies, in accordance with the strategies followed at the national level]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[It is the ratio between the local administrations having adopted and implemented local disaster risk reduction strategies, in accordance with the strategies followed at the national level and the total number of local administrations]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="UNIT_MEASURE">
          
          <com:Text xml:lang="en"><![CDATA[Percentage]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[Divide the number of local governments that have adopted and implemented local disaster risk reduction strategies, in accordance with nationally followed strategies by the total number of local governments, multiply by 100]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[Not available]]></com:Text>
          
        </gen:ReportedAttribute>
        
      </gen:AttributeSet>
    </gen:Report>
  </mes:MetadataSet>
  
