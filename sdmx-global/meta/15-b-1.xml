        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="REL_CAL_POLICY">
          
          <com:Text xml:lang="en"><![CDATA[Annual]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_SOURCE">
          
          <com:Text xml:lang="en"><![CDATA[OCEDE]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COMPILING_ORG">
          
          <com:Text xml:lang="en"><![CDATA[OECD]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="INST_MANDATE">
          
          <com:Text xml:lang="en"><![CDATA[Collect, compile and disseminate data on aid]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="RATIONALE">
          
          <com:Text xml:lang="en"><![CDATA[The indicator is relevant because it reflects the management of aid]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="REC_USE_LIM">
          
          <com:Text xml:lang="en"><![CDATA[The completeness of this indicator depends on regular data collection]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[Determination of the amount of official development assistance and public expenditure devoted to the preservation and sustainable use of biodiversity and ecosystems in millions of USD]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_VALIDATION">
          
          <com:Text xml:lang="en"><![CDATA[Ratio of the area of natural mountain ecosystems found in the country&#39;s protected areas to the total area of the country, expressed as a percentage.]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[Available nationally]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="OTHER_DOC">
          
