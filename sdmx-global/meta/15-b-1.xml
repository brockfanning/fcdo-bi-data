        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_EMAIL">
          
          <com:Text xml:lang="en"><![CDATA[cyrimibu@yahoo.fr]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[This is the amount of public development assistance and public spending devoted to the preservation and sustainable use of biodiversity and ecosystems]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="UNIT_MEASURE">
          
          <com:Text xml:lang="en"><![CDATA[Millions of USD]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SOURCE_TYPE">
          
          <com:Text xml:lang="en"><![CDATA[CNCA]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COLL_METHOD">
          
