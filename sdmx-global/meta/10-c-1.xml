      <gen:AttributeSet>
        
        <gen:ReportedAttribute id="SDG_GOAL">
          
          <com:Text xml:lang="en"><![CDATA[Goal 10. Reduce inequalities within and across countries]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[10.c By 2030, reduce the transaction costs of migrant remittances to below 3 percent and eliminate channels of remittances costing more than 5 percent]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[10.c.1 Costs of remittances in proportion to the amount transferred]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[The amount deducted from the transfers made, set by the BRB according to the amount transferred,]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[Ratio between the sum of all the amounts received following the money transfers made to the total of the transfers]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[Not available]]></com:Text>
          
        </gen:ReportedAttribute>
        
